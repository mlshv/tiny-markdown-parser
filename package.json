{
  "name": "tiny-markdown-parser",
  "version": "1.0.0",
  "description": "Tiny ~1.1kB markdown parser with TypeScript typings",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.module.mjs",
	"exports": "./dist/index.modern.mjs",
  "types": "./tiny-markdown-parser.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "microbundle src/index.js",
    "prepublishOnly": "npm run build",
    "size-limit": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlshv/tiny-markdown-parser.git"
  },
  "keywords": [
    "markdown",
    "parser",
    "tiny",
    "mini",
    "micro",
    "minified",
    "javascript",
    "js"
  ],
  "author": "Mikhail Malyshev <topcatnocap@pm.me>",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "bugs": {
    "url": "https://github.com/mlshv/tiny-markdown-parser/issues"
  },
  "homepage": "https://github.com/mlshv/tiny-markdown-parser",
  "devDependencies": {
    "@size-limit/preset-small-lib": "8.2.4",
    "microbundle": "0.15.1",
    "vitest": "0.31.4"
  },
  "size-limit": [
    {
      "path": "./dist/index.modern.mjs",
      "limit": "1.1 kB"
    }
  ]
}
